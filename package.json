{
  "name": "sequelize-tinker",
  "description": "A REPL for sequelize inspired by Laravel Tinker.",
  "version": "0.2.0",
  "author": "nitaking @nitaking",
  "bin": {
    "sequelize-tinker": "./bin/run"
  },
  "bugs": "https://github.com/nitaking/sequelize-tinker/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2"
  },
  "devDependencies": {
    "@oclif/test": "^1",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "mocha": "^5",
    "nyc": "^13"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/src"
  ],
  "homepage": "https://github.com/nitaking/sequelize-tinker",
  "keywords": [
    "oclif",
    "node",
    "laravel-tinker",
    "rails-console",
    "node-tinker",
    "sequelize",
    "sequelize-tinker"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "bin": "sequelize-tinker"
  },
  "repository": "nitaking/tinker",
  "scripts": {
    "posttest": "eslint src",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "lint:fix": "eslint src --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
